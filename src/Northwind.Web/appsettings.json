{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Products": {
    "DefaultPageSize": 10
  },
  "Categories": {
    "ImageSizeLimitBytes": 28000000,
    "AllowedImageType": "bmp"
  },
  "ActionLogging": {
    "AddArguments": true
  },
  "ConnectionStrings": {
    "Northwind": "Data Source=localhost;Initial Catalog=Northwind;Integrated Security=True"
  }
}
